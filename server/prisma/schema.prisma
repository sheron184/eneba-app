// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  engineType = "binary"
  previewFeatures = ["clientExtensions"]
  moduleFormat="cjs"
}

datasource db {
  provider = "mysql"
}

model Game {
  id        Int      @id @default(autoincrement())
  title     String
  image     String
  price     Float
  createdAt DateTime @default(now())
}
